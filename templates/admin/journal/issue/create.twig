{% extends 'admin/journal/_layout.twig' %}

{% block title %}New Journal Issue{% endblock %}

{% block main %}
  {% embed 'admin/_embed/box.twig' %}
    {% block title %}Create Issue{% endblock %}
    {% block header_right %}
      <div class="input">
        <a href="{{ path('admin_journal_issue_index') }}" class="button"><i class="fas fa-chevron-left"></i> Back to Issues List</a>
      </div>
    {% endblock %}
    {% block body %}
      {% include 'admin/_include/issue/create-update.twig' with { create: true } %}
    {% endblock %}
  {% endembed %}
{% endblock %}
