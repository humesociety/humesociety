{% extends 'admin/content/_layout.twig' %}

{% block title %}Edit Page{% endblock %}

{% block main %}
  {% embed 'admin/_embed/box.twig' %}
    {% block title %}Edit Page: {{ page }}{% endblock %}
    {% block header_right %}
      <div class="input">
        <a href="{{ path('admin_content_page_view') }}" class="button" data-menu="page_section"><i class="fas fa-chevron-left"></i> Back to Page List</a>
      </div>
    {% endblock %}
    {% block body %}
      {% include 'admin/_include/page/create-update.twig' %}
    {% endblock %}
  {% endembed %}
{% endblock %}
